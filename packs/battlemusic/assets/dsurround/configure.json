			]
		},
		"dsurround_battlemusic:battle.miniboss": {
			"category": "music",
			"credits": [
				"§adetermined_persuit_loop.wav",
				"Emma_MA",
				"Creative Commons 0"
			]
		},
		"dsurround_battlemusic:battle.wither": {
			"category": "music",
			"title": "Retribution",
			"credits": [
				"Tanner Helland",
				"Creative Commons Attribution-Share Alike 3.0"
			]
		},
		"dsurround_battlemusic:battle.dragon": {
